<?xml version="1.0" encoding="utf-8"?>
<project>
	
	<meta title="HaxeFlixel Weather Thing" package="dumb.devin503.weather" version="0.0.3" company="devin503" />
	<app main="Main" path="Export" file="FlxWeather" />
	<app preloader="flixel.system.FlxPreloader" />
	
	<source path="Source" />
	
	<haxelib name="lime" />
	<haxelib name="flixel" />
	<haxelib name="flixel-ui" />
	<haxelib name="flixel-addons" />
	<haxelib name="openfl" />
	<haxelib name="weatherapi-hx" />
	
	
	<assets path="Assets/Sounds/Web/" rename="Assets/Sounds/" if="web" />
	<assets path="Assets/Sounds/Sys/" rename="Assets/Sounds/" if="sys" />
	<assets path="Assets/Icons" exclude="*.md" />
	<assets path="Assets/Images" exclude="Eduardo.*" if="web" />
	<assets path="Assets/Placeholders" exclude="*.md" />

	<haxedef name="WINDUSSY" if="debug windows" /> <!-- remind me why i did this -->

	<window width="1368" height="700" fps="" background="#000000" hardware="false" vsync="true" />

	<icon path="Artussy/ballnutters.png" if="debug" />
	<icon path="Artussy/wxIcon.png" unless="debug" />
	
</project>